[
    {
        "name": "jenkins",
        "image": "jenkins",
        "portMappings": [
            {
                "containerPort": 8080,
                "protocol": "tcp"
            }
        ],
        "environment": [
            {
                "name": "JAVA_OPTS",
                "value": "-Xmx1024m -Xms512m"
            }
        ],
        "cpu": 512,
        "memory": 1024,
        "essential": true
    }
]